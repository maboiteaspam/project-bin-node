{
  "name": "<%= global.projectName %>",
  "version": "<%= global.projectVersion %>",
  "description": "to write",
  "main": "<%= global.node_pkg.entry %>",
  "scripts": {
<% if(global.node_pkg.mocha){ %>
    "test": "mocha"
<% } %>
  },
  "bin": {},
  "keywords": <%= JSON.stringify(global.keywords.split(/\S/)) %>,
  "author": "<%= global.author %>"
<% if(global.license){ %>
,"license": "<%= global.license %>"
<% } %>
<% if(global.homepage){ %>
,"homepage": "<%= global.homepage %>"
<% } %>
<% if(global.repository){ %>
,"repository": {
    "url": "https://github.com/<%= global.author %>/<%= global.projectName %>"
  }
<% } %>
<% if(global.bugs){ %>
,"bugs": {
    "url": "https://github.com/<%= global.author %>/<%= global.projectName %>/issues"
  }
<% } %>
}
