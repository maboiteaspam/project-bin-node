{
  "name": "<%= projectName %>",
  "version": "<%= projectVersion %>",
  "description": "<%= description %>",
  "main": "<%= node_pkg.entry %>",
  "scripts": {
<% if(node_pkg.mocha){ %>
    "test": "mocha"
<% } %>
  },
  "bin": {},
  "keywords": <%= JSON.stringify(keywords) %>,
  "author": "<%= author %>"
<% if(license){ %>
,"license": "<%= license %>"
<% } %>
<% if(homepage){ %>
,"homepage": "<%= homepage %>"
<% } %>
<% if(repository){ %>
,"repository": {
    "url": "<%= repository %>"
  }
<% } %>
<% if(bugs){ %>
,"bugs": {
    "url": "<%= bugs %>"
  }
<% } %>
}
